 Cell[BoxData[
  FormBox[
   RowBox[{"3", " ", "\[Times]", " ", "3", " "}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "matrix\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", GridBox[{
      {"a", "b", "c"},
      {"d", "e", "f"},
      {"g", "h", "i"}
     }], ")"}], TraditionalForm]]]
}], "Text",
 CellFrame->{{3, 0}, {0, 0.5}},
