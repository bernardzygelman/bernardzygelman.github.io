Cell[TextData[{
 "Suppose we wish to express the function ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{"sin", "[", "x", "]"}], " ", 
     RowBox[{"Exp", "[", 
      RowBox[{"-", 
       SuperscriptBox["x", "2"]}], "]"}]}]}], TraditionalForm]]],
 " as a Fourier integral"
}], "Text",
 CellFrame->{{3, 0}, {0, 0.5}},
 CellChangeTimes->{{3.747250032423016*^9, 3.747250053133107*^9}, {
   3.747250293567787*^9, 3.747250344174687*^9}, {3.747250410890641*^9, 
   3.747250414581682*^9}, {3.747250455812168*^9, 3.747250460862069*^9}, {
   3.74725077692421*^9, 3.747250780387548*^9}, {3.747252961683589*^9, 
   3.7472529634499407`*^9}, 3.747253363706582*^9}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"fun", "[", "x_", "]"}], "=", 
    RowBox[{
     RowBox[{"Sin", "[", "x", "]"}], 
     RowBox[{"Exp", "[", 
      RowBox[{"-", 
       RowBox[{"x", "^", "2"}]}], "]"}]}]}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"h", "[", "u_", "]"}], "=", 
    RowBox[{"FullSimplify", "[", 
     RowBox[{"FourierTransform", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Sin", "[", "x", "]"}], " ", 
        RowBox[{"Exp", "[", 
         RowBox[{"-", 
          RowBox[{"x", "^", "2"}]}], "]"}]}], ",", "x", ",", "u"}], "]"}], 
     "]"}]}]}]}]], "Input",
 CellChangeTimes->{{3.747250349690895*^9, 3.7472504474854183`*^9}, {
   3.747250752586988*^9, 3.747250797012567*^9}, {3.7472508292119417`*^9, 
   3.747250840910531*^9}, {3.74725107185394*^9, 3.747251072155882*^9}, 
   3.747251111182949*^9, {3.747252952807979*^9, 3.747252955924089*^9}}],

Cell[BoxData[
 RowBox[{
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{"-", 
    SuperscriptBox["x", "2"]}]], " ", 
  RowBox[{"Sin", "[", "x", "]"}]}]], "Output",
 CellChangeTimes->{3.760903786458315*^9}],

Cell[BoxData[
 FractionBox[
  RowBox[{"\[ImaginaryI]", " ", 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     RowBox[{"-", 
      FractionBox["1", "4"]}], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", "+", "u"}], ")"}], "2"]}]], " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"-", "1"}], "+", 
     SuperscriptBox["\[ExponentialE]", "u"]}], ")"}]}], 
  RowBox[{"2", " ", 
   SqrtBox["2"]}]]], "Output",
 CellChangeTimes->{3.760903786700323*^9}]
}, Closed]],

Cell[TextData[{
 "To check whether the above integrand does indeed represent ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], " "}], TraditionalForm]]],
 "when inserted into the integral we construct the following table"
}], "Text",
