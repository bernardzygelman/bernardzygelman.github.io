Cell[TextData[{
 "Notice that the Fourier sum approximates the function ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", "x", ")"}], TraditionalForm]]],
 " ever more accurately as we increase the partial sum index",
 Cell[BoxData[
  FormBox[".", TraditionalForm]]]
}], "Text",
 CellFrame->{{3, 0}, {0, 0.5}},
 CellChangeTimes->{{3.747241941888062*^9, 3.747242047989807*^9}, {
