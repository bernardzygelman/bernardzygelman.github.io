 CellChangeTimes->{{3.74725048800668*^9, 3.7472505072207127`*^9}, {
  3.747250539688511*^9, 3.747250634698614*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"ftable", "[", "x_", "]"}], ":=", 
  RowBox[{"Chop", "[", 
   RowBox[{"NIntegrate", "[", " ", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"h", "[", "u", "]"}], "/", 
       RowBox[{"Sqrt", "[", 
        RowBox[{"2", " ", "Pi"}], "]"}]}], 
      RowBox[{"Exp", "[", 
       RowBox[{
        RowBox[{"-", "I"}], " ", "u", " ", "x"}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"u", ",", 
       RowBox[{"-", "\[Infinity]"}], ",", "\[Infinity]"}], "}"}], ",", 
     RowBox[{"AccuracyGoal", "\[Rule]", "9"}]}], "]"}], "]"}]}]], "Input",
